<archetype-descriptor>
  <requiredProperties>
    <requiredProperty key="defaultjvmserver">
      <defaultValue>defaultjvmserver</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <modules>
    <module name="\${rootArtifactId}-war"
      id="\${rootArtifactId}-war" dir="__rootArtifactId__-war">
      <fileSets>
        <fileSet>
          <directory>src</directory>
          <includes>
            <include>**/*.java</include>
            <include>**/*.xml</include>
            <include>**/*.png</include>
            <include>**/*.html</include>
            <include>**/*.js</include>
            <include>**/*.css</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module name="\${rootArtifactId}-bundle"
      id="\${rootArtifactId}-bundle" dir="__rootArtifactId__-bundle">
    </module>
  </modules>
</archetype-descriptor>